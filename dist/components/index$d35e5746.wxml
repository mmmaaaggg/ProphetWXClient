<import src="echarts$2c664bef" />
<template name="index$d35e5746">
  <view class="_div data-v-90a90d1c container">
    <scroll-view scroll-y="true" class="_scroll-view data-v-90a90d1c scroll">
      <view class="_div data-v-90a90d1c chartContainer">
        <template data="{{...$root[$kk+'0'], $root}}" is="echarts$2c664bef"></template>
      </view>
      <view class="_div data-v-90a90d1c tapContainer trTap">
        <view class="_div data-v-90a90d1c blog vertify">
          <view class="_div data-v-90a90d1c wxcIcon vertifyIcon">
            <wxc-icon size="45" type="feedback" class="_wxc-icon data-v-90a90d1c feedback"></wxc-icon>
          </view>
          <view class="_div data-v-90a90d1c bg-right" bindtap="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}">
            <view class="_div data-v-90a90d1c bg-item digita">{{confirmData}}</view>
            <view class="_div data-v-90a90d1c bg-item blogitem">待验证</view>
          </view>
        </view>
        <view class="_div data-v-90a90d1c blog vertified">
          <view class="_div data-v-90a90d1c wxcIcon vertifiedIcon">
            <wxc-icon size="45" type="rate" class="_wxc-icon data-v-90a90d1c rate"></wxc-icon>
          </view>
          <view class="_div data-v-90a90d1c bg-right" bindtap="handleProxy" data-eventid="{{'1'}}" data-comkey="{{$k}}">
            <view class="_div data-v-90a90d1c bg-item digita">{{vertified}}</view>
            <view class="_div data-v-90a90d1c bg-item blogitem">已验证</view>
          </view>
        </view>
        <view class="_div data-v-90a90d1c blog attention">
          <view class="_div data-v-90a90d1c wxcIcon attentionIcon">
            <wxc-icon size="45" type="star" class="_wxc-icon data-v-90a90d1c star"></wxc-icon>
          </view>
          <view class="_div data-v-90a90d1c bg-right">
            <view class="_div data-v-90a90d1c bg-item digita">{{interest}}</view>
            <view class="_div data-v-90a90d1c bg-item blogitem">关注预言</view>
          </view>
        </view>
      </view>
      <view class="_div data-v-90a90d1c tbContainer">
        <view class="_div data-v-90a90d1c tr bg-t">
          <view class="_div data-v-90a90d1c th th-name">预测名称</view>
          <view class="_div data-v-90a90d1c th th-date">起止日期</view>
          <view class="_div data-v-90a90d1c th th-rate">准确率</view>
          <view class="_div data-v-90a90d1c th th-person">预言家</view>
        </view>
        <view wx:key="index" key="{{index}}" class="_div data-v-90a90d1c tr bg-c" wx:for="{{listData}}" wx:for-index="index" wx:for-item="items">
          <view class="_div data-v-90a90d1c td name th-name"> {{items.name}} </view>
          <view class="_div data-v-90a90d1c td th-date">
            <view class="_div data-v-90a90d1c td-date">
              <view class="_div data-v-90a90d1c date">{{items.date_from}}</view>
              <view class="_div data-v-90a90d1c date">{{items.date_to}}</view>
            </view>
          </view>
          <view class="_div data-v-90a90d1c td th-rate"> {{items.cmp_id}} </view>
          <view class="_div data-v-90a90d1c td th-person">
            <view class="_div data-v-90a90d1c username"> {{items.username}} </view>
            <view class="_div data-v-90a90d1c button-collection" bindtap="handleProxy" data-eventid="{{'2-'+index}}" data-comkey="{{$k}}">
              <wxc-icon wx:if="{{collect[index]}}" size="40" type="star-active" class="_wxc-icon data-v-90a90d1c collected"></wxc-icon>
              <wxc-icon wx:else size="40" type="star" class="_wxc-icon data-v-90a90d1c collected"></wxc-icon>
            </view>
          </view>
        </view>
      </view>
    </scroll-view>
  </view>
</template>